<% ['n_bedrooms', 'unit_number', 'sqft'].each do |field_name| %>
  <div class="form-row">
    <label class="form-row__label">
      <%= field_name.gsub('_', ' ').titleize %>
    </label>
    <input disabled="disabled" type="text" value="<%= physical_structure.send(field_name.to_sym) %>">
  </div>
<% end %>
